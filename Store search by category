function DLookup1($fld, $tab, $whr)
{
	$q = "Select $fld from $tab where $whr";
	//echo $q;
	$rst = mysql_query($q);	
	if ($row = mysql_fetch_object($rst)) 
		return $row->$fld;
	else
		return NULL;
}

